claimsprocess=# SELECT pid,
       usename,
       state,
       backend_xid,
       backend_xmin,
       xact_start,
       query_start,
       wait_event_type,
       wait_event,
       query
FROM pg_stat_activity
WHERE backend_xmin IS NOT NULL
  AND backend_xmin::text = '240491835'
ORDER BY xact_start;
