Most safe approach

-- 1. Add new TIMESTAMP column
ALTER TABLE dur_tmp_02 ADD COLUMN prcs_add_ts TIMESTAMP;

-- 2. Populate it (only valid rows)
UPDATE dur_tmp_02
SET prcs_add_ts = prcs_add_tmsp::TIMESTAMP
WHERE prcs_add_tmsp ~ '^\d{4}-\d{2}-\d{2} \d{2}:\d{2}:\d{2}(\.\d+)?$';

-- 3. (Optional) Check how many failed to convert
SELECT COUNT(*) FROM dur_tmp_02 WHERE prcs_add_tmsp IS NOT NULL AND prcs_add_ts IS NULL;

